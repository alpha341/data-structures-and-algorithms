#include<bits/stdc++.h>
using namespace std;
void printvec(vector<pair<int,string> >);
int main()
{
  int n;
  string x; 
  vector<pair<int,string> >v={{1,"ben"}};//vector of pairs
  v.push_back(make_pair(2,"siva"));//method 1 adding elemets
  v.pop_back();
  printvec(v);
  //
  cout<<"enter number and name";
  cin>>n>>x;
  v.push_back({n,x});//method 2 of adding elemnst
  printvec(v);
}
void printvec(vector<pair<int,string> >v)
{
  for(int i=0;i<v.size();i++)
  {
    cout<<v[i].first<<" "<<v[i].second<<endl;;
  }

}

